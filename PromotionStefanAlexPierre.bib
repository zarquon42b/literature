  pg        = {8},
  pi        = {BOLOGNA},
  publisher = {Edizioni Centauro},
  sc        = {Neurosciences; Neuroimaging; Radiology, Nuclear Medicine & MedicalEOLEOLImaging},
  sn        = {1120-9976},
