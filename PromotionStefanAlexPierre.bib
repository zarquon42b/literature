  groups      = {Orbita},
  issn        = {0941-293X},
  issue       = {5},
  keyword     = {Medicine},
  keywords    = {Eye},
  owner       = {ruedell},
  publisher   = {Springer Berlin / Heidelberg},
  timestamp   = {2010.10.15},
  url         = {http://dx.doi.org/10.1007/s003470170126},
}

