  Url                      = {http://www.sciencedirect.com/science/article/pii/S0047248481800590}
}

@Article{Moshfeghi1994,
  author    = {Moshfeghi, M and Ranganath, S and Nawyn, K},
  title     = {Three-dimensional elastic matching of volumes},
