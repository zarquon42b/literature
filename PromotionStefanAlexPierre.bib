  Keywords                 = {Acrocephalosyndactylia, pathology; Adolescent; Aging, pathology; Animals; Cebus, anatomy /&/ histology; Child, Preschool; Craniofacial Dysostosis, pathology; Facial Bones, anatomy /&/ histology/pathology; Female; Humans; Male; Models, Biological; Sex Characteristics; Skull, anatomy /&/ histology/pathology},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {schlager},
  Pmid                     = {1746646},
  Timestamp                = {2013.02.15},
  Url                      = {http://dx.doi.org/10.1002/ajpa.1330860307}
}

@Article{Rohlf1984,
