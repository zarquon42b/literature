  Timestamp                = {2013.12.09},
  Url                      = {http://dx.doi.org/10.1111/1556-4029.12328}
}

@Article{Evteev2013,
  author      = {Evteev, Andrej and Cardini, Andrea L. and Morozova, Irina and O'Higgins, Paul},
  title       = {Extreme climate, rather than population history, explains mid-facial morphology of northern asians.},
  journal     = {American Journal of Physical Anthropology},
  year        = {2014},
  volume      = {153},
  number      = {3},
  pages       = {449--462},
  month       = {Mar},
  abstract    = {Previous studies have examined mid-facial cold adaptation among either widely dispersed and genetically very diverse groups of humans isolated for tens of thousands of years, or among very closely related groups spread over climatically different regions. Here we present a study of one East Asian and seven North Asian populations in which we examine the evidence for convergent adaptations of the mid-face to a very cold climate. Our findings indicate that mid-facial morphology is strongly associated with climatic variables that contrast the temperate climate of East Asians and the very cold and dry climate of North Asians. This is also the case when either maxillary or nasal cavity measurements are considered alone. The association remains significant when mtDNA distances among populations are taken into account. The morphological contrasts between populations are consistent with physiological predictions and prior studies of mid-facial cold adaptation in more temperate regions, but among North Asians there appear to be some previously undescribed morphological features that might be considered as adaptive to extreme cold. To investigate this further, analyses of the seven North Asian populations alone suggest that mid-facial morphology remains strongly associated with climate, particularly winter precipitation, contrasting coastal Arctic and continental climates. However, the residual covariation among North Asian mid-facial morphology and climate when genetic distances are considered, is not significant. These findings point to modern adaptations to extreme climate that might be relevant to our understanding of the mid-facial morphology of fossil hominins that lived during glaciations. Am J Phys Anthropol 153:449-462, 2014. {\copyright} 2013 Wiley Periodicals, Inc.},
  doi         = {10.1002/ajpa.22444},
  file        = {:PDF/EvteevEtAl-ExtremeClimateRatherThanPopulation.pdf:PDF},
  groups      = {Climate adaptation},
  institution = {Anuchin Research Institute and Museum of Anthropology, Lomonosov Moscow State University, Moscow, 125009, Russia.},
  issn        = {1096-8644},
  keywords    = {respiratory adaptation, mid-facial morphology, population genetics, Northern Asia, nose, Nose, Nase, nasal},
  owner       = {ruedell},
  timestamp   = {2013.12.04},
  url         = {http://dx.doi.org/10.1002/ajpa.22444},
}

