@Article{Kendall1980,
  author   = {Kendall, David G. and Kendall, Wilfrid S.},
  title    = {Alignments in Two-Dimensional Random Sets of Points},
  journal  = {Advances in Applied Probability},
  year     = {1980},
  volume   = {12},
  number   = {2},
  pages    = {380-424},
  note     = {ArticleType: primary_article / Full publication date: Jun., 1980 / Copyright 1980 Applied Probability Trust},
  abstract = {Let n points in the plane be generated by some specified random mechanism and suppose that N({\^I}{\textmu}) of the $\left( \smallmatrix n \\ 3 \endsmallmatrix \right)$ resulting triads form triangles with largest angle -{\^I}{\textmu} . The main object of the paper is to obtain asymptotic formulae for EN({\^I}{\textmu}) and Var(N({\^I}{\textmu})) when $\varepsilon \downarrow 0$ , and to solve the associated data-analytic problem of testing whether an empirical set of n points should be considered to contain too many such {\^I}{\textmu} -blunt triads in the situation where the generating mechanism is unknown and where all that can be said about the tolerance {\^I}{\textmu} is that it must be allowed to take values anywhere in a given interval (T0,T1) $(0<T_{0}<T_{1})$ . This problem is solved by the introduction of a plot to be called the pontogram and by the introduction of simulation-based significance tests constructed by random lateral perturbations of the data.},
  file     = {:PDF\\KendallKendall-AlignmentsTwoDimRandSetsPoints.pdf:PDF},
  groups   = {Methods},
}

@Article{Kendall1984,
