@OTHER{Schmittbuhl2001,
  __markedentry = {[ruedell]},
  abstract = {A new quantitative approach of the mandibular sexual dimorphism, based
	on computer-aided image analysis and elliptical Fourier analysis
	of the mandibular outline in lateral view is presented. This method
	was applied to a series of 117 dentulous mandibles from 69 male and
	48 female individuals native of Rhenish countries. Statistical discriminant
	analysis of the elliptical Fourier harmonics allowed the demonstration
	of a significant sexual dimorphism in 97.1% of males and 91.7% of
