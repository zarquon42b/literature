@Article{Riede2010,
  author    = {Riede, Moritz and Schueppel, Rico and Sylvester-Hvid, Kristian O. and K{\"u}hne, Martin and R{\"o}ttger, Michael C. and Zimmermann, Klaus and Liehr, Andreas W.},
