  Keywords                 = {LTS5; Segmentation; Active contours; Multimodal imaging; 3D Ultrasound imaging; Radiotherapy planning; Radiotherapy},
  Location                 = {Porto, Portugal},
  Oai-id                   = {oai:infoscience.epfl.ch:138683},
  Oai-set                  = {conf; fulltext-public; fulltext},
  Owner                    = {ruedell},
  Review                   = {REVIEWED},
