@Article{Tang2014,
  author    = {Kuangyun Tang and Yuchun Hsu and Jing Hu and Yan Zhao and Qiushi Wang and Jihua Li},
  title     = {New horizontal v-shaped osteotomy for correction of protrusion of the zygoma and the zygomatic arch in East Asians: indication and results},
  journal   = {British Journal of Oral and Maxillofacial Surgery},
