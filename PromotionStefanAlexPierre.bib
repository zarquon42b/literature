}

@Article{Schmittbuhl1999,
  author    = {Schmittbuhl, Matthieu and Le Minor, Jean-Marie and Allenbach, Bernard and Schaaf, A.},
  title     = {Shape of the orbital opening: individual characterization and analysis of variability in modern humans, Gorilla gorilla, and Pan troglodytes},
  journal   = {Annals of Anatomy - Anatomischer Anzeiger},
  year      = {1999},
  volume    = {181},
  number    = {3},
  pages     = {299--307},
  month     = may,
  abstract  = {Summary The description of the human orbital shape is principally qualitative in the classical literature, and characterised by adjectives such as circular, rectangular or quadrangular. In order to provide a precise quantification and interpretation of this shape, a study based on automatic image analysis and Fourier analysis was carried out on 45 human skulls (30 males, 15 females), and for comparison on 61 skulls of Gorilla gorilla (40 males, 21 females), and 34 skulls of Pan troglodytes (20 males, 14 females). Sexual dimorphism in the shape of the orbital opening was not demonstrated. Its dominant morphological features could be characterized by Fourier analysis; elliptical elongation and quadrangularity were dominant morphological features of the shape of the orbital opening in the three species. Elliptical elongation was more marked in humans and Pan, whereas quadrangularity was particularly emphasized in Gorilla. An intraspecific variability of the shape of the orbital opening existed in humans, Gorilla and Pan, and seemed close in the three species. Interspecific partition between humans, Gorilla and Pan was demonstrated despite the variability observed in the three species studied. Interspecific differences between Gorilla and the Pan-humans group were principally explained by the differences in quadrangularity, and by differences in orientation of triangularity and pentagonality. Differences in the shape of the orbital opening between humans and Pan were principally explained by differences in hexagonality, and by differences in orientation of quadrangularity. A closeness of shape between some humans and some individuals in Pan and, to a lesser degree, with some individuals in Gorilla was observed, demonstrating the existence of a morphological continuum of the shape of the orbital opening in hominoids.},
  file      = {:PDF/SchmittbuhlEtAl-ShapeOfTheOrbitalOpening.pdf:PDF},
  groups    = {Orbita},
  issn      = {0940-9602},
  keywords  = {Orbit, Face, Hominoids, Fourier analysis, Image analysis},
  owner     = {ruedell},
  timestamp = {2011.03.03},
  url       = {http://www.sciencedirect.com/science/article/B7GWX-4GWPJG6-H/2/d171266645aa6431933ce012cc011f41},
}

@Article{Achene2000,
