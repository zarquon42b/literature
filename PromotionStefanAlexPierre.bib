@ARTICLE{Vernot2014,
  author = {Vernot, Benjamin and Akey, Joshua M.},
  title = {Resurrecting Surviving Neandertal Lineages from Modern Human Genomes},
  journal = {Science},
  year = {2014},
  abstract = {Anatomically modern humans overlapped and mated with Neandertals such
	that non-African humans inherit ~1-3% of their genomes from Neandertal
	ancestors. We identified Neandertal lineages that persist in the
	DNA of modern humans, in whole-genome sequences from 379 European
	and 286 East Asian individuals, recovering over 15 Gb of introgressed
	sequence that spans ~20% of the Neandertal genome (FDR = 5%). Analyses
	of surviving archaic lineages suggests that there were fitness costs
	to hybridization, admixture occurred both before and subsequent to
	divergence of non-African modern humans, and Neandertals were a source
