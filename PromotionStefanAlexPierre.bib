@InCollection{Cardini2010,
  Title                    = {Biogeographic Analysis Using Geometric Morphometrics: Clines in Skull Size and Shape in a Widespread African Arboreal Monkey},
  Author                   = {Cardini, Andrea and Filho, Jos{\'e} and Polly, P. and Elton, Sarah},
  Booktitle                = {Morphometrics for Nonmorphometricians},
