@Article{1992,
  author              = {Muller, Keith E. and LaVange, Lisa M. and Sharon Landesman Ramey and Ramey, Craig T.},
  title               = {Power Calculations for General Linear Multivariate Models Including Repeated Measures Applications},
  journal             = {Journal of the American Statistical Association},
  year                = {1992},
  volume              = {87},
  number              = {420},
  pages               = {1209-1226},
  abstract            = {Recently developed methods for power analysis expand the options available for study design. We demonstrate how easily the methods can be applied by (1) reviewing their formulation and (2) describing their application in the preparation of a particular grant proposal. The focus is a complex but ubiquitous setting: repeated measures in a longitudinal study. Describing the development of the research proposal allows demonstrating the steps needed to conduct an effective power analysis. Discussion of the example also highlights issues that typically must be considered in designing a study. First, we discuss the motivation for using detailed power calculations, focusing on multivariate methods in particular. Second, we survey available methods for the general linear multivariate model (GLMM) with Gaussian errors and recommend those based on F approximations. The treatment includes coverage of the multivariate and univariate approaches to repeated measures, MANOVA, ANOVA, multivariate regression, and univariate regression. Third, we describe the design of the power analysis for the example, a longitudinal study of a child's intellectual performance as a function of mother's estimated verbal intelligence. Fourth, we present the results of the power calculations. Fifth, we evaluate the tradeoffs in using reduced designs and tests to simplify power calculations. Finally, we discuss the benefits and costs of power analysis in the practice of statistics. We make three recommendations: 1. Align the design and hypothesis of the power analysis with the planned data analysis, as best as practical. 2. Embed any power analysis in a defensible sensitivity analysis. 3. Have the extent of the power analysis reflect the ethical, scientific, and monetary costs. We conclude that power analysis catalyzes the interaction of statisticians and subject matter specialists. Using the recent advances for power analysis in linear models can further invigorate the interaction.},
  copyright           = {Copyright {\copyright} 1992 American Statistical Association},
  file                = {:PDF/Muller-Power.Calculations.for.General.Linear.pdf:PDF},
  groups              = {Statistics},
  issn                = {01621459},
  jstor_articletype   = {research-article},
  jstor_formatteddate = {Dec., 1992},
  language            = {English},
  owner               = {schlager},
  publisher           = {American Statistical Association},
  timestamp           = {2011.07.08},
  url                 = {http://www.jstor.org/stable/2290663},
}

@Article{Muller1984143,
  author    = {Keith E. Muller and Bercedis L. Peterson},
  title     = {Practical methods for computing power in testing the multivariate general linear hypothesis},
  journal   = {Computational Statistics \& Data Analysis},
  year      = {1984},
  volume    = {2},
  number    = {2},
  pages     = {143 - 158},
  abstract  = {Four test statistics are commonly used in multivariate general linear hypothesis tests such as MANOVA: Roy's largest root, Wilks' Lambda, Hotelling-Lawley trace and Pillai-Bartlett trace. Closed form, finite series expressions do not exist for the distribution functions in either the null or non-null case (except for special cases). In practice, asymptotic approximations, based on F or chi square distributions, are used for the null case. It is not widely known that similarly accurate and general approximations have been published for the non-null case, for all except the largest root [15,38]. In this paper new approximations, based on noncentral F's, are provided for power for all but the largest root. These generalize existing F approximations for the central case. Much less calculation is needed than for earlier approximations and accuracy appears not to suffer in practice. An upper bound F approximation is provided for the largest root power. Power calculation can be used to help choose a test statistic as well as for experimental design. A particularly convenient method for estimating power, using standard linear models programs [20], is generalized to the multivariate case.},
  doi       = {DOI: 10.1016/0167-9473(84)90002-1},
  file      = {:PDF/Muller-Practical.methods.for.computing.power.pdf:PDF},
  groups    = {Statistics},
  issn      = {0167-9473},
  keywords  = {MANOVA},
  owner     = {schlager},
  timestamp = {2011.07.08},
  url       = {http://www.sciencedirect.com/science/article/pii/0167947384900021},
}

