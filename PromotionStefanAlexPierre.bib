@Article{Ruedell2013,
  author       = {Alexandra R{\"u}dell},
  title        = {Size matters - effects of allometry on sexual dimorphism of the human orbital region},
  journal      = {10. Internationaler Kongress der Gesellschaft f{\"u}r Anthropologie (GfA), Bozen, Italien},
  year         = {2013},
  abstract     = {Shape and size differences of the human orbital region due to sex and population affinity are crucial in physical anthropology. Especially in the context of forensics anthropometric measurements are used to find out sex and origin of the deceased. With classical anthropometric methods, though, it is not possible to distinguish between differences in size and shape. Furthermore, using the orbital index, only a minimal aspect of the shape of the orbital rim can be grasped. Hence the question remains to what extent sexual dimorphism in the orbital region is influenced by size differences between male and female individuals and how pure shape differences are expressed. The influence of population affinity on sexual dimorphism is of high interest, too. In this study, 200 surface meshes derived from CT-scans of the orbital region are analysed with the methods of Geometric Morphometrics. The data consists of evenly distributed male and female individuals of Chinese and European origin. The orbital region as a whole is analysed in contrast to a subregion representing the orbital rim. The results are compared to other studies concerning the sexual dimorphism represented in the human orbital region. It can be shown that it heavily depends on the chosen method of representing and analysing the orbital region what conclusion can be drawn.},
  groups       = {PublicationsRuedell},
  howpublished = {10th international GfA meeting, Biological anthropology: Prospects and Perspectives, 2md - 6th September 2013},
  owner        = {ruedell},
  timestamp    = {2015.02.22},
}

