  journal  = {American Journal of Physical Anthropology},
  year     = {2008},
  volume   = {137},
  number   = {2},
  pages    = {201-212},
  note     = {ISI Document Delivery No.: 350LP Times Cited: 0 Cited Reference Count: 65 Miller, Steven F. White, Jessica L. Ciochon, Russell L.},
  abstract = {This study provides a survey of mandibular shape in a sample of extant hominoids (Pan, Gorilla, Pongo, and Hylobates), as well as extinct Asian and Eurasian taxa (Ouranopithecus, Sivapithecus, and Gigantopithecus) in order to compare overall shape similarity. Results presented call into question differences in mandible shape recently used to distinguish Gigantopithecus giganteus from Gigantopithecus blacki and to justify resurrecting a different generic designation, "Indopithecus," for the former. It is concluded that while the two large-bodied Asian taxa may have been adapted to slightly different dietary niches with different geographic and temporal ranges, the unique mandibular/dental characters that the two taxa share should not be viewed as independent evolutionary developments.},
  groups   = {Allometry},
  keywords = {relative warps analysis allometry diet cluster analysis G. blacki G. giganteus TRIDIMENSIONAL PROCRUSTES ANALYSIS ELLIPTIC FOURIER-ANALYSIS FUNCTIONAL-SIGNIFICANCE SOUTH CHINA HOMO-ERECTUS BUBING BASIN AFRICAN APES GREAT APE SYMPHYSIS TOOTH},
}

@Article{Mitteroecker2004,
  Title                    = {Comparison of cranial ontogenetic trajectories among great apes and humans},
  Author                   = {Mitteroecker, P. and Gunz, P. and Bernhard, M. and Schaefer, K. and Bookstein, F. L.},
  Journal                  = {Journal of Human Evolution},
  Year                     = {2004},
  Note                     = {Mitteroecker, Philipp Gunz, Philipp Bernhard, Markus Schaefer, Katrin Bookstein, Fred L Comparative Study Research Support, Non-U.S. Gov't England Journal of human evolution J Hum Evol. 2004 Jun;46(6):679-97.},
  Number                   = {6},
  Pages                    = {679-97},
  Volume                   = {46},
  Abstract                 = {Molecular data suggest that humans are more closely related to chimpanzees than either is to the gorillas, yet one finds the closest similarity in craniofacial morphology to be among the great apes to the exclusion of humans. To clarify how and when these differences arise in ontogeny, we studied ontogenetic trajectories for Homo sapiens, Pan paniscus, Pan troglodytes, Gorilla gorilla and Pongo pygmaeus. A total of 96 traditional three-dimensional landmarks and semilandmarks on the face and cranial base were collected on 268 adult and sub-adult crania for a geometric morphometric analysis. The ontogenetic trajectories are compared by various techniques, including a new method, relative warps in size-shape space. We find that adult Homo sapiens specimens are clearly separated from the great apes in shape space and size-shape space. Around birth, Homo sapiens infants are already markedly different from the great apes, which overlap at this age but diverge among themselves postnatally. The results suggest that the small genetic differences between Homo and Pan affect early human ontogeny to induce the distinct adult human craniofacial morphology. Pure heterochrony does not sufficiently explain the human craniofacial morphology nor the differences among the African apes.},
  File                     = {:PDF\\Miteroecker- Comparison of cranial trajectories.pdf:PDF},
  Keywords                 = {Adult Animals Anthropometry/methods Child Female Gorilla gorilla/anatomy & histology/growth & development Hominidae/*anatomy & histology/*growth & development Humans Infant Male Pan paniscus/anatomy & histology/growth & development Pan troglodytes/anatomy & histology/growth & development Pongo pygmaeus/anatomy & histology/growth & development Skull/*anatomy & histology/*growth & development}
}

