@ARTICLE{Coquerelle2011,
  author = {Coquerelle, Michael and Bookstein, Fred L. and Braga, José and Halazonetis,
	Demetrios J. and Weber, Gerhard W. and Mitteroecker, Philipp},
  title = {Sexual dimorphism of the human mandible and its association with
