@Article{Lou2007,
  author   = {Lou, Leo and Lagravere, Manuel O. and Compton, Sharon and Major, Paul W. and Flores-Mir, Carlos},
  title    = {Accuracy of measurements and reliability of landmark identification with computed tomography (CT) techniques in the maxillofacial area: a systematic review},
  journal  = {Oral Surgery, Oral Medicine, Oral Pathology, Oral Radiology, and Endodontology},
