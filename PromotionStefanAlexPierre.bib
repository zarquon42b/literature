@Article{Wilson1981,
  Title                    = {On comparing fossil specimens with population samples},
  Author                   = {S.R. Wilson},
  Journal                  = {Journal of Human Evolution},
  Year                     = {1981},
  Number                   = {3},
  Pages                    = {207 - 214},
  Volume                   = {10},
  Abstract                 = {In comparing individual specimens with population samples, there may be many aspects of the comparison in which one is interested. One may wish to allocate the individual to one of the populations, or one may wish to calculate the atypicality of the individual with respect to each of the populations (based on Hotelling's T2) or one may wish to determine the (log-) odds of the individual belonging to one of two populations. As well, one may be interested in judging how likely it is that a single specimen may belong to one or more populations. So a distance measure is proposed, based on the dicrimination technique that determines more accurate and realistic (log-) odds estimates. This gives rise to a comparatively interactive, and exploratory, analysis of the data. Two examples are given.},
  Doi                      = {10.1016/S0047-2484(81)80059-0},
  File                     = {:PDF/wilson1981.pdf:PDF},
